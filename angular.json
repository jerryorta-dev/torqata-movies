{
  "version": 1,
  "projects": {
    "movies": {
      "projectType": "application",
      "root": "apps/movies",
      "sourceRoot": "apps/movies/src",
      "prefix": "torqata-movies",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "apps/firebase/public",
            "index": "apps/movies/src/index.html",
            "main": "apps/movies/src/main.ts",
            "polyfills": "apps/movies/src/polyfills.ts",
            "tsConfig": "apps/movies/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/movies/src/favicon.ico",
              "apps/movies/src/assets"
            ],
            "styles": [
              "apps/movies/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/shared/theme/src/lib/theming/light-theme"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/movies/src/environments/environment.ts",
                  "with": "apps/movies/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "movies:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "movies:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "movies:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/movies/src/**/*.ts",
              "apps/movies/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/movies"
          ],
          "options": {
            "jestConfig": "apps/movies/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "movies-e2e": {
      "root": "apps/movies-e2e",
      "sourceRoot": "apps/movies-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/movies-e2e/cypress.json",
            "tsConfig": "apps/movies-e2e/tsconfig.e2e.json",
            "devServerTarget": "movies:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "movies:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/movies-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
